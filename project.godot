config_version=5

[application]

config/name="Hemovintner"
config/description="A Gothic modern vampire game combining bar management with action-platformer gameplay"
run/main_scene="res://scenes/test_simple.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://assets/icons/icon.svg"

[autoload]

GameManager="*res://autoloads/game_manager.gd"
DataManager="*res://autoloads/data_manager.gd"
InventorySystem="*res://autoloads/inventory_system.gd"
EconomySystem="*res://autoloads/economy_system.gd"
CraftingSystem="*res://autoloads/crafting_system.gd"
PlayerData="*res://autoloads/player_data.gd"
SceneManager="*res://autoloads/scene_manager.gd"
PatronManager="*res://autoloads/patron_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

